git_url(ENV["mw_cert_git_url"])

type("development") # The default type, can be: appstore, adhoc, enterprise or development

app_identifier([ENV["mw_app_identifier"]])
username(ENV["mw_apple_id"]) # Your Apple Developer Portal username

